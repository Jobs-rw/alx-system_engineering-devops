#!/usr/bin/env bash
# Connects to server 18.205.38.219 with key in ~/.sh
#!/bin/bash

# Define the server hostname or IP address
server="your_server_hostname_or_ip"

# Define the path to the private key file
private_key="~/.ssh/school"

# Use SSH with single-character flags to connect
ssh -i "$private_key" "ubuntu@$server"
